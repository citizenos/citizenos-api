    "campaign-mustache": "1.0.0",
    "campaign-nodemailer": "1.1.0",
    "campaign-terminal": "1.0.4",
    "config": "3.3.6",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "docx": "6.0.3",
    "etherpad-lite-client": "git+https://github.com/citizenos/etherpad-lite-client-js#86e06b78f159f1abc7916ee5c817adb096a951f5",
    "express": "4.17.1",
    "express-device": "0.4.2",
