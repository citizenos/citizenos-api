    "fe": "https://app.citizenos.com"
  },
  "db": {
    "url": "postgres://citizenos:citizenos@localhost:5432/citizenos",
    "options": {
      "dialect": "postgres",
      "logging": true,
      "benchmark": true,
      "sync": {
        "force": false,
        "logging": true
      },
      "define": {
        "underscored": false,
        "freezeTableName": false,
        "syncOnAssociation": false,
        "charset": "utf8",
        "collate": "utf8_general_ci",
        "timestamps": true,
        "paranoid": true
      },
      "omitNull": false,
      "dialectOptions": {
        "ssl": true
      },
      "pool": {
        "min": 0,
        "max": 100
      },
      "isolationLevel": "READ COMMITTED",
      "operatorsAliases": false
    }
  },
  "topic": {
    "titleLengthMax": 1000
  },
